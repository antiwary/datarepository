{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adltrainingdatafactory"
		},
		"ADLAzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADLAzureSqlDatabase'"
		},
		"ADLAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLAzureDataLakeStorage'"
		},
		"ADLAzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adltrainingkv.vault.azure.net/"
		},
		"ADLAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlstoragedls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLAzureSqlDatabase_connectionString": {
						"value": "[parameters('ADLAzureSqlDatabase_connectionString')]"
					},
					"ADLAzureDataLakeStorage_accountKey": {
						"value": "[parameters('ADLAzureDataLakeStorage_accountKey')]"
					},
					"ADLAzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('ADLAzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"ADLAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ADLAzureDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}